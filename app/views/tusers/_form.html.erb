<%= form_with(model: @user, url: users_path, local: true) do |form| %>
  <div>
    <%= form.label :nombre %>
    <%= form.text_field :nombre %>
  </div>

  <div>
    <%= form.label :educacion %>
    <%= form.text_field :educacion %>
  </div>

  <div>
    <%= form.label :habilidades %>
    <%= form.text_area :habilidades %>
  </div>

  <div>
    <%= form.label :experiencia %>
    <%= form.text_area :experiencia %>
  </div>

  <div>
    <%= form.label :email %>
    <%= form.email_field :email %>
  </div>

  <div>
    <%= form.label :password %>
    <%= form.password_field :password %>
  </div>

  <div>
    <%= form.label :password_confirmation %>
    <%= form.password_field :password_confirmation %>
  </div>

  <div>
    <%= form.submit %>
  </div>
<% end %>